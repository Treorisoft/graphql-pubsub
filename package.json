{
  "name": "@treorisoft/graphql-pubsub",
  "version": "1.1.0",
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/Treorisoft/graphql-pubsub",
  "scripts": {
    "copy-env": "",
    "start": "",
    "build:esm": "tsc -b tsconfig.esm.json && node scripts/esm-post-process.mjs",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:script": "yarpm run build:esm && yarpm run build:cjs",
    "build:clean": "rimraf lib",
    "build": "yarpm run build:clean && yarpm run build:script",
    "test": ""
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./lib/client": {
      "types": "./lib/client.d.ts",
      "require": "./lib/client.js",
      "import": "./lib/client.mjs"
    }
  },
  "dependencies": {
    "bluebird": "~3.7.2"
  },
  "devDependencies": {
    "@apollo/client": "~3.8.4",
    "@types/node": "~20.10.8",
    "glob": "^11.0.0",
    "rimraf": "~5.0.5",
    "typescript": "~5.3.3",
    "@types/bluebird": "~3.5.42",
    "yarpm": "~1.2.0"
  },
  "peerDependencies": {
    "graphql": "^15.7.2 || ^16.0.0",
    "graphql-ws": "~5.14.3",
    "ioredis": "~5.4.1"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  }
}
